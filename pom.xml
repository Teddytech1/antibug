<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>npc</groupId>
  <artifactId>antibug</artifactId>
  <version>0.3</version>
  <packaging>jar</packaging>
  <name>antibug</name>
  <description>JUnit Extension Tools.</description>
  <dependencies>
    <dependency>
      <groupId>jdk.tools</groupId>
      <artifactId>jdk.tools</artifactId>
      <version>8</version>
      <scope>system</scope>
      <systemPath>${java.home}/../lib/tools.jar</systemPath>
      <exclusions/>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.12</version>
      <scope>compile</scope>
      <exclusions/>
    </dependency>
    <dependency>
      <groupId>npc</groupId>
      <artifactId>sinobu</artifactId>
      <version>1.0</version>
      <scope>compile</scope>
      <exclusions/>
    </dependency>
  </dependencies>
  <licenses>
    <license>
      <name>The MIT License</name>
      <url>https://opensource.org/licenses/MIT</url>
    </license>
  </licenses>
  <repositories/>
  <url>https://github.com/Teletha/AntiBug</url>
  <scm>
    <url>https://github.com/Teletha/AntiBug</url>
    <connection>scm:git:https://github.com/Teletha/AntiBug.git</connection>
    <developerConnection>scm:git:https://github.com/Teletha/AntiBug.git</developerConnection>
  </scm>
  <issueManagement>
    <system>GitHub</system>
    <url>https://github.com/Teletha/AntiBug/issues</url>
  </issueManagement>
  <developers>
    <developer>
      <name>Teletha</name>
      <email/>
      <url>https://github.com/Teletha</url>
    </developer>
  </developers>
  <properties>
    <maven.compiler.source>8</maven.compiler.source>
    <maven.compiler.target>8</maven.compiler.target>
    <maven.compiler.encoding>UTF-8</maven.compiler.encoding>
  </properties>
</project>
